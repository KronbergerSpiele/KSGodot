      <HintPath>../.mono/assemblies/Debug/GodotSharp.dll</HintPath>
      <HintPath>../.mono/assemblies/Release/GodotSharp.dll</HintPath>
    </Reference>
    <PackageReference Update="FSharp.Core" Version="5.0.0"  />
  </ItemGroup>
</Project>